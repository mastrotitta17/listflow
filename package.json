{
  "name": "listflow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:init": "npx supabase init",
    "db:link": "node -e \"const {spawnSync}=require('child_process'); const ref=process.env.SUPABASE_PROJECT_REF; if(!ref){console.error('Missing SUPABASE_PROJECT_REF'); process.exit(1);} const res=spawnSync('npx',['supabase','link','--project-ref',ref],{stdio:'inherit',shell:true}); process.exit(res.status ?? 1);\"",
    "db:new": "npx supabase migration new",
    "db:push": "npx supabase db push",
    "db:pull": "npx supabase db pull",
    "db:types": "npx supabase gen types typescript --linked --schema public > lib/supabase/database.types.ts",
    "db:seed:categories": "node scripts/seed-categories-from-static.cjs",
    "db:backfill:category-en": "node scripts/backfill-category-en-titles.cjs"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.84.0",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crisp-sdk-web": "^1.0.27",
    "framer-motion": "^12.34.3",
    "lucide-react": "^0.575.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.2.3",
    "recharts": "^3.7.0",
    "stripe": "^20.3.1",
    "tailwind-merge": "^3.5.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
